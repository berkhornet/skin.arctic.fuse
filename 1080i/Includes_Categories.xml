<?xml version="1.0" encoding="UTF-8"?>
<includes>
    <include name="_Categories_Selector_PreItemFade">
        <animation effect="fade" start="100" end="0" delay="100" time="300" tween="quadratic" reversible="false" condition="!Container($PARAM[id]).OnNext">Conditional</animation>
    </include>

    <include name="Categories_Selector_Movement_Position">
        <focusposition>0</focusposition>
        <movement>3</movement>
    </include>

    <include name="Categories_Selector_Movement_Submenu">
        <include content="Categories_Selector_Movement_Position">
            <param name="id">$PARAM[id]</param>
        </include>
        <width>920</width>
    </include>
    <include name="Categories_Selector_Movement_Mainmenu">
        <include content="Categories_Selector_Movement_Position">
            <param name="id">$PARAM[id]</param>
        </include>
        <width>680</width>
    </include>

    <include name="Categories_Selector">
        <param name="id">300</param>
        <param name="window_id" />
        <param name="slide">False</param>
        <param name="font">font_main_black</param>
        <param name="color_fo">main_fg_100</param>
        <param name="color_nf">main_fg_30</param>
        <param name="hidden_button">List_ButtonMenu_Row_HiddenButton_Home</param>
        <param name="shorten">False</param>
        <param name="preitem_fade">True</param>
        <param name="hitrect_x">0</param>
        <param name="hitrect_y">0</param>
        <param name="hitrect_w">0</param>
        <param name="hitrect_h">0</param>
        <definition>
            <include content="List_ButtonMenu_Row">
                <param name="id">$PARAM[id]</param>
                <param name="color_fo">$PARAM[color_fo]</param>
                <param name="color_nf">$PARAM[color_nf]</param>
                <param name="itemlayout_w">160</param>
                <param name="offset_x">view_button_offset_x</param>
                <param name="control">fixedlist</param>
                <param name="font">$PARAM[font]</param>
                <param name="backing">true</param>
                <param name="hidden_button">$PARAM[hidden_button]</param>
                <param name="hidden_button_prefix">$PARAM[hidden_button_prefix]</param>
                <preloaditems>0</preloaditems>
                <scrolltime tween="quadratic">0</scrolltime>

                <hitrect x="$PARAM[hitrect_x]" y="$PARAM[hitrect_y]" w="$PARAM[hitrect_w]" h="$PARAM[hitrect_h]" />

                <nested />

                <include condition="![$PARAM[shorten]]" content="Categories_Selector_Movement_Submenu">
                    <param name="id">$PARAM[id]</param>
                </include>
                <include condition="$PARAM[shorten]" content="Categories_Selector_Movement_Mainmenu">
                    <param name="id">$PARAM[id]</param>
                </include>

                <onfocus condition="$PARAM[slide]">SetProperty(Slide.WidgetContainer,$PARAM[id],$PARAM[window_id])</onfocus>
                <onfocus condition="![$PARAM[slide]]">ClearProperty(Slide.WidgetContainer,$PARAM[window_id])</onfocus>

                <left>160</left>
                <height>hub_categorieswidget_h</height>
                <top>hub_categorieswidget_t</top>
            </include>

        </definition>
    </include>

    <include name="Categories_Selector_Fake">
        <param name="id">302</param>
        <param name="font">font_mini</param>
        <param name="textoffsety">109</param>
        <param name="textcolor_nf">main_fg_30</param>
        <param name="textcolor_fo">main_fg_90</param>
        <param name="visible_five">false</param>
        <definition>
            <control type="grouplist">
                <height>256</height>
                <orientation>horizontal</orientation>
                <itemgap>-80</itemgap>
                <animation effect="slide" start="0" end="-64" time="400" tween="quadratic" easing="out" condition="!Container($PARAM[id]).OnScrollNext" reversible="false">Conditional</animation>
                <animation effect="slide" start="0" end="64" time="400" tween="quadratic" easing="out" condition="!Container($PARAM[id]).OnScrollPrevious" reversible="false">Conditional</animation>
                <control type="button">
                    <width>auto</width>
                    <height>256</height>
                    <centertop>50%</centertop>
                    <aligny>top</aligny>
                    <textoffsety>$PARAM[textoffsety]</textoffsety>
                    <include>Texture_MainMenu_Highlight_ToggleButton_FakeFocus</include>
                    <texturenofocus />
                    <font>$PARAM[font]</font>
                    <!-- <shadowcolor>main_bg_30</shadowcolor> -->
                    <textcolor>$PARAM[textcolor_nf]</textcolor>
                    <label>$INFO[Container($PARAM[id]).ListItemNoWrap(-3).Label]</label>
                    <textoffsetx>80</textoffsetx>
                    <onclick>SetFocus($PARAM[id],-3)</onclick>
                    <onclick>Action(Select)</onclick>
                    <visible>Container($PARAM[id]).Position(3)</visible>
                    <visible>!String.IsEmpty(Container($PARAM[id]).ListItemNoWrap(-3).CurrentItem)</visible>
                    <!-- <visible>!Integer.IsEqual(Container($PARAM[id]).NumItems,0)</visible>
                    <visible>!Integer.IsEqual(Container($PARAM[id]).NumItems,1)</visible>
                    <visible>!Integer.IsEqual(Container($PARAM[id]).NumItems,2)</visible>
                    <visible>String.IsEmpty(Container($PARAM[id]).ListItemNoWrap(1).CurrentItem)</visible>
                    <visible>!String.IsEmpty(Container($PARAM[id]).ListItemNoWrap(-3).CurrentItem)</visible> -->
                </control>
                <control type="button">
                    <width>auto</width>
                    <height>256</height>
                    <centertop>50%</centertop>
                    <aligny>top</aligny>
                    <textoffsety>$PARAM[textoffsety]</textoffsety>
                    <include>Texture_MainMenu_Highlight_ToggleButton_FakeFocus</include>
                    <texturenofocus />
                    <font>$PARAM[font]</font>
                    <!-- <shadowcolor>main_bg_30</shadowcolor> -->
                    <textcolor>$PARAM[textcolor_nf]</textcolor>
                    <label>$INFO[Container($PARAM[id]).ListItemNoWrap(-2).Label]</label>
                    <textoffsetx>80</textoffsetx>
                    <onclick>SetFocus($PARAM[id],-2)</onclick>
                    <onclick>Action(Select)</onclick>
                    <visible>Container($PARAM[id]).Position(3) | Container($PARAM[id]).Position(2)</visible>
                    <visible>!String.IsEmpty(Container($PARAM[id]).ListItemNoWrap(-2).CurrentItem)</visible>
                    <!-- <visible>!Integer.IsEqual(Container($PARAM[id]).NumItems,0)</visible>
                    <visible>!Integer.IsEqual(Container($PARAM[id]).NumItems,1)</visible>
                    <visible>!Integer.IsEqual(Container($PARAM[id]).NumItems,2)</visible>
                    <visible>String.IsEmpty(Container($PARAM[id]).ListItemNoWrap(2).CurrentItem)</visible>
                    <visible>!String.IsEmpty(Container($PARAM[id]).ListItemNoWrap(-2).CurrentItem)</visible> -->
                </control>
                <control type="button">
                    <width>auto</width>
                    <height>256</height>
                    <centertop>50%</centertop>
                    <aligny>top</aligny>
                    <textoffsety>$PARAM[textoffsety]</textoffsety>
                    <include>Texture_MainMenu_Highlight_ToggleButton_FakeFocus</include>
                    <texturenofocus />
                    <font>$PARAM[font]</font>
                    <!-- <shadowcolor>main_bg_30</shadowcolor> -->
                    <textcolor>$PARAM[textcolor_nf]</textcolor>
                    <label>$INFO[Container($PARAM[id]).ListItemNoWrap(-1).Label]</label>
                    <textoffsetx>80</textoffsetx>
                    <onclick>SetFocus($PARAM[id],-1)</onclick>
                    <onclick>Action(Select)</onclick>
                    <visible>Container($PARAM[id]).Position(3) | Container($PARAM[id]).Position(2) | Container($PARAM[id]).Position(1)</visible>
                    <visible>!String.IsEmpty(Container($PARAM[id]).ListItemNoWrap(-1).CurrentItem)</visible>
                    <!-- <visible>!Integer.IsEqual(Container($PARAM[id]).NumItems,0)</visible>
                    <visible>!Integer.IsEqual(Container($PARAM[id]).NumItems,1)</visible>
                    <visible>String.IsEmpty(Container($PARAM[id]).ListItemNoWrap(3).CurrentItem)</visible>
                    <visible>!String.IsEmpty(Container($PARAM[id]).ListItemNoWrap(-1).CurrentItem)</visible> -->
                </control>
                <control type="togglebutton">
                    <width>auto</width>
                    <height>256</height>
                    <centertop>50%</centertop>
                    <aligny>top</aligny>
                    <textoffsety>$PARAM[textoffsety]</textoffsety>
                    <include>Texture_MainMenu_Highlight_ToggleButton_FakeFocus</include>
                    <usealttexture>Control.HasFocus($PARAM[id])</usealttexture>
                    <font>$PARAM[font]</font>
                    <!-- <shadowcolor>main_bg_30</shadowcolor> -->
                    <textcolor>$PARAM[textcolor_fo]</textcolor>
                    <label>$INFO[Container($PARAM[id]).ListItemNoWrap(0).Label]</label>
                    <textoffsetx>80</textoffsetx>
                    <altlabel>[COLOR=$VAR[ColorSelected]]$INFO[Container($PARAM[id]).ListItemNoWrap(0).Label][/COLOR]</altlabel>
                    <onclick>SetFocus($PARAM[id],0)</onclick>
                    <onclick>Action(Select)</onclick>
                </control>
                <control type="button">
                    <width>auto</width>
                    <height>256</height>
                    <centertop>50%</centertop>
                    <aligny>top</aligny>
                    <textoffsety>$PARAM[textoffsety]</textoffsety>
                    <include>Texture_MainMenu_Highlight_ToggleButton_FakeFocus</include>
                    <texturenofocus />
                    <font>$PARAM[font]</font>
                    <!-- <shadowcolor>main_bg_30</shadowcolor> -->
                    <textcolor>$PARAM[textcolor_nf]</textcolor>
                    <label>$INFO[Container($PARAM[id]).ListItemNoWrap(1).Label]</label>
                    <textoffsetx>80</textoffsetx>
                    <onclick>SetFocus($PARAM[id],1)</onclick>
                    <onclick>Action(Select)</onclick>
                    <visible>Container($PARAM[id]).Position(2) | Container($PARAM[id]).Position(1) | Container($PARAM[id]).Position(0)</visible>
                    <visible>!String.IsEmpty(Container($PARAM[id]).ListItemNoWrap(1).CurrentItem)</visible>
                    <!-- <visible>!Integer.IsEqual(Container($PARAM[id]).NumItems,0)</visible>
                    <visible>!Integer.IsEqual(Container($PARAM[id]).NumItems,1)</visible>
                    <visible>!String.IsEmpty(Container($PARAM[id]).ListItemNoWrap(1).CurrentItem)</visible> -->
                </control>
                <control type="button">
                    <width>auto</width>
                    <height>256</height>
                    <centertop>50%</centertop>
                    <aligny>top</aligny>
                    <textoffsety>$PARAM[textoffsety]</textoffsety>
                    <include>Texture_MainMenu_Highlight_ToggleButton_FakeFocus</include>
                    <texturenofocus />
                    <font>$PARAM[font]</font>
                    <!-- <shadowcolor>main_bg_30</shadowcolor> -->
                    <textcolor>$PARAM[textcolor_nf]</textcolor>
                    <label>$INFO[Container($PARAM[id]).ListItemNoWrap(2).Label]</label>
                    <textoffsetx>80</textoffsetx>
                    <onclick>SetFocus($PARAM[id],2)</onclick>
                    <onclick>Action(Select)</onclick>
                    <visible>Container($PARAM[id]).Position(1) | Container($PARAM[id]).Position(0)</visible>
                    <visible>!String.IsEmpty(Container($PARAM[id]).ListItemNoWrap(2).CurrentItem)</visible>
                    <!-- <visible>!Integer.IsEqual(Container($PARAM[id]).NumItems,0)</visible>
                    <visible>!Integer.IsEqual(Container($PARAM[id]).NumItems,1)</visible>
                    <visible>!Integer.IsEqual(Container($PARAM[id]).NumItems,2)</visible>
                    <visible>!String.IsEmpty(Container($PARAM[id]).ListItemNoWrap(2).CurrentItem)</visible> -->
                </control>
                <control type="button">
                    <width>auto</width>
                    <height>256</height>
                    <centertop>50%</centertop>
                    <aligny>top</aligny>
                    <textoffsety>$PARAM[textoffsety]</textoffsety>
                    <include>Texture_MainMenu_Highlight_ToggleButton_FakeFocus</include>
                    <texturenofocus />
                    <font>$PARAM[font]</font>
                    <!-- <shadowcolor>main_bg_30</shadowcolor> -->
                    <textcolor>$PARAM[textcolor_nf]</textcolor>
                    <label>$INFO[Container($PARAM[id]).ListItemNoWrap(3).Label]</label>
                    <textoffsetx>80</textoffsetx>
                    <onclick>SetFocus($PARAM[id],3)</onclick>
                    <onclick>Action(Select)</onclick>
                    <visible>Container($PARAM[id]).Position(0) | [$PARAM[visible_five] + Container($PARAM[id]).Position(1)]</visible>
                    <visible>!String.IsEmpty(Container($PARAM[id]).ListItemNoWrap(3).CurrentItem)</visible>
                    <!-- <visible>!Integer.IsEqual(Container($PARAM[id]).NumItems,0)</visible>
                    <visible>!Integer.IsEqual(Container($PARAM[id]).NumItems,1)</visible>
                    <visible>!Integer.IsEqual(Container($PARAM[id]).NumItems,2)</visible>
                    <visible>!Integer.IsEqual(Container($PARAM[id]).NumItems,3)</visible>
                    <visible>!String.IsEmpty(Container($PARAM[id]).ListItemNoWrap(3).CurrentItem)</visible> -->
                </control>
                <control type="button">
                    <width>auto</width>
                    <height>256</height>
                    <centertop>50%</centertop>
                    <aligny>top</aligny>
                    <textoffsety>$PARAM[textoffsety]</textoffsety>
                    <include>Texture_MainMenu_Highlight_ToggleButton_FakeFocus</include>
                    <texturenofocus />
                    <font>$PARAM[font]</font>
                    <!-- <shadowcolor>main_bg_30</shadowcolor> -->
                    <textcolor>$PARAM[textcolor_nf]</textcolor>
                    <label>$INFO[Container($PARAM[id]).ListItemNoWrap(4).Label]</label>
                    <textoffsetx>80</textoffsetx>
                    <onclick>SetFocus($PARAM[id],4)</onclick>
                    <onclick>Action(Select)</onclick>
                    <visible>Container($PARAM[id]).Position(0)</visible>
                    <visible>$PARAM[visible_five]</visible>
                    <visible>!String.IsEmpty(Container($PARAM[id]).ListItemNoWrap(4).CurrentItem)</visible>
                    <!-- <visible>!Integer.IsEqual(Container($PARAM[id]).NumItems,0)</visible>
                    <visible>!Integer.IsEqual(Container($PARAM[id]).NumItems,1)</visible>
                    <visible>!Integer.IsEqual(Container($PARAM[id]).NumItems,2)</visible>
                    <visible>!Integer.IsEqual(Container($PARAM[id]).NumItems,3)</visible>
                    <visible>!String.IsEmpty(Container($PARAM[id]).ListItemNoWrap(3).CurrentItem)</visible> -->
                </control>
            </control>
        </definition>
    </include>
</includes>