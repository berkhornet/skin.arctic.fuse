{
    "comments": "Static Power Side Menu",
    "template": "generator/data/parts/basic_include.xmltemplate",
    "includes_name": "skinvariables-powersidemenu-staticitems",
    "includes_part": {
        "menu": "powersidemenu",
        "for_each": [
            {
                "template": "generator/data/parts/dialog_sideitem.xmltemplate",
                "datafile": "generator/data/setup/onclick_path.json",
                "condition": ["{item_disabled}!=True", "{item_label}!=", "{item_label}!=$VAR[Label_Setting_Shortcuts_Locked]"]
            }
        ]
    }
}