{
    "comments": "Static Power Main Menu",
    "template": "generator/data/parts/basic_include.xmltemplate",
    "includes_name": "skinvariables-powermainmenu-staticitems",
    "includes_part": {
        "menu": "powermainmenu",
        "for_each": [
            {
                "template": "generator/data/parts/dialog_item.xmltemplate",
                "datafile": [
                    "generator/data/setup/onclick_path.json",
                    "generator/data/setup/powermainmenu_item.json"
                ],
                "condition": [
                    "{item_disabled}!=True",
                    "{item_label}!="
                ]
            }
        ]
    }
}