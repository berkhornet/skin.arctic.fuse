{
    "comments": "Static Sidemenu",
    "template": "generator/data/parts/basic_include.xmltemplate",
    "includes_name": "skinvariables-sidemenu-staticitems",
    "includes_part": {
        "menu": "sidemenu",
        "for_each": [
            {
                "template": "generator/data/parts/menu_item.xmltemplate",
                "datafile": "generator/data/setup/onclick_path.json",
                "condition": "{item_disabled}!=True",
                "widget_group_id": [
                    {
                        "condition": "{item_global_widgets}==True",
                        "value": "99"
                    },
                    "{item_x}"
                ],
                "parts_additional": {"template": "generator/data/parts/additional_sidemenu.xmltemplate"}
            }
        ]
    }
}